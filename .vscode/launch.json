    "version": "0.2.0",
    "configurations": [
        {
            "name": "Chrome (requires https://pub.dev/packages/flutter_cors)",
            "request": "launch",
            "type": "dart",
            "program": "lib/main_app.dart",
            "args": [
                "-d",
                "chrome",
            ],
        },
        {
            "name": "Windows",
            "request": "launch",
            "type": "dart",
            "program": "lib/main_app.dart",
            "args": [
                "-d",
                "windows",
            ]
        },
        {
            "name": "typingthon (profile mode)",
            "request": "launch",
            "type": "dart",
            "flutterMode": "profile"
        }
    ]
}